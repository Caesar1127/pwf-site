<div class="page-header">
  <h1>Year End Reports</h1>
</div>
<table class="dtable table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Student</th>
      <th>Season</th>
      <th>Confirmed</th>
      <th>Date Submitted</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @year_end_reports.each do |rep| %>
      <tr>
        <td><%=link_to(rep.name, mgr_year_end_report_path(rep)) %></td>
        <td><%= rep.student_name %></td>
        <td><%= rep.term %></td>
        <td><%= yesno(rep.confirmed) %></td>
        <td><%=submission_date(rep) %></td>
        <td><%report_collection_links(rep)%> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%=new_resource_link%>

