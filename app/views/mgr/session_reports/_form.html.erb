<%= simple_form_for([:mgr,@session_report], :html => { :class => 'form-horizontal report', :novalidate=>"" }) do |f|%>

  <%=f.association :tutoring_assignment, collection: mgr_assignments_for_select(@assignments), label: "Assignment", input_html:{class: "jqSelect"}%>
  <%=render :partial => "shared/session_report_shared_fields", locals:{f:f}%>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary', :id=>"save" %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                mgr_session_reports_path, :class => 'btn' %>
  </div>

<% end %>
