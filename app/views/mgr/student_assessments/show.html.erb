<%- model_class = StudentAssessment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:aep_registration_id) %>:</strong></dt>
  <dd><%= @student_assessment.aep_registration_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pre_test_grade) %>:</strong></dt>
  <dd><%= @student_assessment.pre_test_grade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pre_test_date) %>:</strong></dt>
  <dd><%= @student_assessment.pre_test_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:post_test_grade) %>:</strong></dt>
  <dd><%= @student_assessment.post_test_grade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:post_test_date) %>:</strong></dt>
  <dd><%= @student_assessment.post_test_date %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              mgr_student_assessments_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_mgr_student_assessment_path(@student_assessment), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mgr_student_assessment_path(@student_assessment),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
