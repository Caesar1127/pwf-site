<div class="page-header">
  <h1>AEP Session History</h1>
</div>
<table class="dtable table table-striped">
  <thead>
    <tr>
       <th>Date</th>
      <th>Season</th>
      <th>Total Enrollment</th>
      <th>Present</th>
      <th>Actions</th>
 
    </tr>
  </thead>
  <tbody>
    <% @aep_sessions.each do |aep_session| %>
      <tr>
        <td><%= link_to format_date(aep_session.session_date), edit_mgr_aep_session_path(aep_session) %></td>
        <td><%= aep_session.term %></td>
        <td><%= aep_session.enrollment_count %></td>
        <td><%=aep_session.attendees %></td>
        <td>
          <%= link_to "Edit" , edit_mgr_aep_session_path(aep_session), :class => 'btn btn-mini' %>
          <%= link_to "Destroy",
                      mgr_aep_session_path(aep_session),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_mgr_aep_session_path,
            :class => 'btn btn-primary' %>
