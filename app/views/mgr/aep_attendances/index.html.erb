<%- model_class = AepAttendance -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:aep_registration_id) %></th>
      <th><%= model_class.human_attribute_name(:aep_session_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @aep_attendances.each do |aep_attendance| %>
      <tr>
        <td><%= link_to aep_attendance.id, mgr_aep_attendance_path(aep_attendance) %></td>
        <td><%= aep_attendance.aep_registration_id %></td>
        <td><%= aep_attendance.aep_session_id %></td>
        <td><%=l aep_attendance.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_mgr_aep_attendance_path(aep_attendance), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      mgr_aep_attendance_path(aep_attendance),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_mgr_aep_attendance_path,
            :class => 'btn btn-primary' %>
