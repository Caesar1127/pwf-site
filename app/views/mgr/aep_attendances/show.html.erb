<%- model_class = AepAttendance -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:aep_registration_id) %>:</strong></dt>
  <dd><%= @aep_attendance.aep_registration_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:aep_session_id) %>:</strong></dt>
  <dd><%= @aep_attendance.aep_session_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              mgr_aep_attendances_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_mgr_aep_attendance_path(@aep_attendance), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mgr_aep_attendance_path(@aep_attendance),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
