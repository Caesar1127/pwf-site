<%- model_class = AepRegistration -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:student_registration_id) %>:</strong></dt>
  <dd><%= @aep_registration.student_registration_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:payment_id) %>:</strong></dt>
  <dd><%= @aep_registration.payment_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:learning_disability) %>:</strong></dt>
  <dd><%= @aep_registration.learning_disability %></dd>
  <dt><strong><%= model_class.human_attribute_name(:iep) %>:</strong></dt>
  <dd><%= @aep_registration.iep %></dd>
  <dt><strong><%= model_class.human_attribute_name(:student_academic_contract) %>:</strong></dt>
  <dd><%= @aep_registration.student_academic_contract %></dd>
  <dt><strong><%= model_class.human_attribute_name(:parent_participation_agreement) %>:</strong></dt>
  <dd><%= @aep_registration.parent_participation_agreement %></dd>
  <dt><strong><%= model_class.human_attribute_name(:transcript_test_score_release) %>:</strong></dt>
  <dd><%= @aep_registration.transcript_test_score_release %></dd>
  <dt><strong><%= model_class.human_attribute_name(:learning_disability_details) %>:</strong></dt>
  <dd><%= @aep_registration.learning_disability_details %></dd>
  <dt><strong><%= model_class.human_attribute_name(:iep_details) %>:</strong></dt>
  <dd><%= @aep_registration.iep_details %></dd>
  <dt><strong><%= model_class.human_attribute_name(:season_id) %>:</strong></dt>
  <dd><%= @aep_registration.season_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:payment_status_cd) %>:</strong></dt>
  <dd><%= @aep_registration.payment_status_cd %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              mgr_aep_registrations_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_mgr_aep_registration_path(@aep_registration), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mgr_aep_registration_path(@aep_registration),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
