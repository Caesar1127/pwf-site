<%- model_class = YearEndReport -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="dtable table table-striped">
  <thead>
    <tr>
      <th>Student</th>
      <th>Season</th>
      <th>Confirmed</th>
      <th>Date Submitted</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @year_end_reports.each do |rep| %>
      <tr>
        <td><%= rep.student_name %></td>
        <td><%= rep.term %></td>
        <td><%= yesno(rep.confirmed) %></td>
        <td><%=submission_date(rep) %></td>
        <td><%report_collection_links(rep)%> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%=new_resource_link%>

