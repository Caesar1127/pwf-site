<%- model_class = WorkshopEnrollment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:workshop_id) %>:</strong></dt>
  <dd><%= @workshop_enrollment.workshop_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:aep_registration_id) %>:</strong></dt>
  <dd><%= @workshop_enrollment.aep_registration_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status_cd) %>:</strong></dt>
  <dd><%= @workshop_enrollment.status_cd %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              workshop_enrollments_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_workshop_enrollment_path(@workshop_enrollment), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              workshop_enrollment_path(@workshop_enrollment),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
