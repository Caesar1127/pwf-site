<%=render "shared/session_report_show" %>
<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              session_reports_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_session_report_path(@session_report), :class => 'btn' unless @session_report.confirmed?%>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              session_report_path(@session_report),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' unless @session_report.confirmed? %>
</div>
